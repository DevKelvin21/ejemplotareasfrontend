{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: 'HelloWorld',\n  data: function data() {\n    return {\n      error: false,\n      tareas: [],\n      nuevaTarea: {\n        \"nombre\": \"\",\n        \"hecho\": false\n      }\n    };\n  },\n  methods: {\n    cargarDatos: function cargarDatos() {\n      var thisVue = this;\n      this.axios.get('/tareas').then(function (response) {\n        //console.log(response.data)\n        thisVue.tareas = [];\n        thisVue.tareas = response.data;\n      }).catch(function (error) {\n        console.log(error);\n        thisVue.error = true;\n      });\n    },\n    agregarTarea: function agregarTarea() {\n      var thisVue = this;\n      this.axios.post('/tareas', thisVue.nuevaTarea).then(function () {\n        thisVue.cargarDatos();\n      }).catch(function (error) {\n        console.log(error);\n        thisVue.error = true;\n      });\n      thisVue.nuevaTarea.nombre = \"\";\n    },\n    eliminarTarea: function eliminarTarea(ltarea) {\n      console.log(ltarea);\n      return;\n      var thisVue = this;\n      this.axios.post('/tareas', thisVue.nuevaTarea).then(function () {\n        thisVue.cargarDatos();\n      }).catch(function (error) {\n        console.log(error);\n        thisVue.error = true;\n      });\n      thisVue.nuevaTarea.nombre = \"\";\n    }\n  },\n  mounted: function mounted() {\n    this.cargarDatos();\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuFA;AACAA,oBADA;AAEAC;AAAA;AACAC,kBADA;AAEAC,gBAFA;AAGAC;AACA,oBADA;AAEA;AAFA;AAHA;AAAA,GAFA;AAUAC;AACAC,eADA,yBACA;AACA;AACA,gCACAC,IADA,CACA;AACA;AACAC;AACAA;AACA,OALA,EAMAC,KANA,CAMA;AACAC;AACAF;AACA,OATA;AAUA,KAbA;AAcAG,gBAdA,0BAcA;AACA;AACA,qDACAJ,IADA,CACA;AACAC;AACA,OAHA,EAIAC,KAJA,CAIA;AACAC;AACAF;AACA,OAPA;AAQAA;AACA,KAzBA;AA0BAI,iBA1BA,yBA0BAC,MA1BA,EA0BA;AACAH;AACA;AACA;AACA,qDACAH,IADA,CACA;AACAC;AACA,OAHA,EAIAC,KAJA,CAIA;AACAC;AACAF;AACA,OAPA;AAQAA;AACA;AAvCA,GAVA;AAmDAM,SAnDA,qBAmDA;AACA;AACA;AArDA","names":["name","data","error","tareas","nuevaTarea","methods","cargarDatos","then","thisVue","catch","console","agregarTarea","eliminarTarea","ltarea","mounted"],"sourceRoot":"src/components","sources":["HelloWorld.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row class=\"text-center\">\n      \n      <v-col>\n        <h1 class=\"display-2 font-weight-bold mb-3\">\n          Bienvenido!!\n        </h1>\n\n        <p class=\"subheading font-weight-regular\">\n          Front End del sistema de tareas,<br> para utilizar como ejemplo en las practicas de despliegue.\n          \n        </p>\n      </v-col>\n\n      <v-col cols=\"12\">\n        <v-alert v-show=\"error\" shaped prominent type=\"error\" >\n          Error al intentar conectar con el backend, revise el archivo main.js en el se encuentra la propiedad baseURL la cual tiene la direccion principal del backend. Revise y vuelva a realizar Build\n        </v-alert>\n        <h2 class=\"headline font-weight-bold mb-3\">\n          Agregar:\n        </h2>\n\n        <v-row justify=\"center\">\n\n         <v-col cols=\"4\">\n          <v-text-field\n            label=\"Nueva Tarea:\"\n            outlined\n            v-model=\"nuevaTarea.nombre\"\n          ></v-text-field>\n      </v-col> \n      <v-col cols=\"1\" class=\"mt-2\">\n        <v-btn :disabled=\"!nuevaTarea.nombre\" color=\"primary\" @click=\"agregarTarea\">Agregar</v-btn>\n      </v-col>\n\n          \n\n        </v-row>\n      </v-col>\n\n      <v-col\n        class=\"mb-5\"\n        cols=\"12\"\n      >\n        <h2 class=\"headline font-weight-bold mb-3\">\n          Tareas\n        </h2>\n        <v-row justify=\"center\" >\n          <v-card v-for=\"tarea in tareas\" \n          :key=\"tarea.id\"\n          max-width=\"400\"\n          color=\"grey lighten-2\"\n          class=\"ma-3\"\n          >            \n              <v-col >\n                <v-checkbox\n                  v-model=\"tarea.hecho\"\n                  :label=\"tarea.nombre\"\n                ></v-checkbox>\n                <v-col>\n                  <v-btn\n                    fab\n                    dark\n                    small\n                    color=\"primary\"\n                  >\n                    <v-icon dark>\n                      mdi-minus\n                    </v-icon>\n                  </v-btn>\n                </v-col>              \n                \n              </v-col>\n          </v-card>\n          \n          </v-row>\n        \n      </v-col>\n\n      \n    </v-row>\n  </v-container>\n</template>\n\n<script>\n\n  export default {\n    name: 'HelloWorld',\n    data: () => ({\n      error:false,\n      tareas:[],\n      nuevaTarea:{\n        \"nombre\": \"\",\n        \"hecho\": false\n      },\n    }),\n    methods:{\n      cargarDatos(){\n        let thisVue=this\n        this.axios.get('/tareas')\n              .then(function(response){\n                //console.log(response.data)\n                thisVue.tareas=[]\n                thisVue.tareas=response.data                \n              })\n              .catch((error)=>{\n                console.log(error)\n                thisVue.error=true\n              })\n      },\n      agregarTarea(){\n        let thisVue=this\n        this.axios.post('/tareas',thisVue.nuevaTarea)\n              .then(function(){\n                thisVue.cargarDatos()             \n              })\n              .catch((error)=>{\n                console.log(error)\n                thisVue.error=true\n              })\n        thisVue.nuevaTarea.nombre=\"\"\n      },\n      eliminarTarea(ltarea){\n        console.log(ltarea)\n        return\n        let thisVue=this\n        this.axios.post('/tareas',thisVue.nuevaTarea)\n              .then(function(){\n                thisVue.cargarDatos()             \n              })\n              .catch((error)=>{\n                console.log(error)\n                thisVue.error=true\n              })\n        thisVue.nuevaTarea.nombre=\"\"\n      },\n    },\n    mounted(){\n      this.cargarDatos()\n    }   \n  }\n</script>\n"]},"metadata":{},"sourceType":"module"}